{
  "watch": {
    "stylelint-dev": {
      "patterns": [
        "/src/**/*.scss"
      ],
      "extensions": "scss"
    },
    "eslint": {
      "patterns": [
        "/src/**/*.js"
      ],
      "extensions": "js"
    }
  },
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "all": "concurrently \"(npm run server --prefix ../)\" \"(npm run start)\"",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-sass-js start-js",
    "build": "npm run eslint && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "stylelint-dev": "stylelint \"src/**/*.scss\" --cache --cache-location \".stylelintcache\" exit 0",
    "stylelint-build": "stylelint src/**/*.scss",
    "stylelint-fix-sass": "stylelint --fix src/**/*.scss",
    "watch-sass-js": "npm-watch",
    "eslint": "node_modules/.bin/eslint src/",
    "precommit": "lint-staged"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/pickers": "^3.2.10",
    "@material-ui/styles": "^4.10.0",
    "@material-ui/system": "^4.9.14",
    "axios": "^0.20.0",
    "date-fns": "^2.16.1",
    "formik": "^2.2.1",
    "formik-material-ui": "^3.0.0",
    "formik-material-ui-lab": "0.0.7",
    "formik-material-ui-pickers": "0.0.11",
    "history": "^5.0.0",
    "html-react-parser": "^0.14.1",
    "immutability-helper": "^3.1.1",
    "jwt-decode": "^3.1.2",
    "material": "^0.4.3",
    "material-ui-search-bar": "^1.0.0",
    "node-sass": "^4.14.1",
    "notistack": "^1.0.1",
    "qs": "^6.9.4",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-html-parser": "^2.0.2",
    "react-infinite-scroll-component": "^5.1.0",
    "react-redux": "^7.2.1",
    "react-responsive-carousel": "^3.2.10",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.4",
    "react-slick": "^0.27.13",
    "react-text-mask": "^5.4.3",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "slick-carousel": "^1.8.1",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "npm-run-all": "^4.1.1",
    "npm-watch": "^0.3.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.2",
    "prettier": "^2.1.2",
    "standard": "^14.3.4",
    "stylelint": "^13.7.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-order": "^4.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js, jsx}": [
      "node_modules/.bin/eslint --max-warnings=0",
      "prettier --write"
    ]
  },

  "proxy": "http://localhost:5000"
}
